{
	"expo": {
		"name": "Shopping Cart",
		"slug": "shopping_cart",
		"privacy": "public",
		"sdkVersion": "36.0.0",
		"platforms": ["ios", "android", "web"],
		"version": "4.0.0",
		"orientation": "portrait",
		"icon": "./assets/sc_icon.png",
		"splash": {
			"image": "./assets/sc_transparent.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"notification": {
			"icon": "./assets/sc_transparent_96_96.png"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true
		},
		"androidStatusBar": {
			"backgroundColor": "#ffffff",
			"barStyle": "dark-content"
		},
		"android": {
			"package": "pt.fhenriques.shopping_cart",
			"versionCode": 4,
			"config": {
				"googleMobileAdsAppId": "${process.env.GOOGLE_MOBILE_ADS_APP_ID}",
				"googleSignIn": {
					"apiKey": "${GOOGLE_SIGN_IN_API_KEY}",
					"certificateHash": "${GOOGLE_SIGN_IN_CERTIFICATE_HASH}"
				}
			}
		}
	}
}
